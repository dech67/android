<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    tools:ignore="HardcodedText"
    tools:context=".customer_report.Car_view_fragment">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:orientation="horizontal"
            style="?buttonBarStyle"
            android:weightSum="3">
            <Button
                android:id="@+id/bt_cvstepney"
                android:layout_width="0dp"
                android:layout_height="40dp"
                android:layout_margin="4dp"
                android:layout_weight="1"
                style="?buttonBarButtonStyle"
                android:background="@drawable/customer_rprt_bt01"
                android:fontFamily="@font/montserrat_medium1"
                android:text="Stepney"
                android:textAllCaps="false"
                android:textSize="12sp" />

            <Button
                android:id="@+id/bt_cvtoolkit"
                android:layout_width="0dp"
                android:layout_height="40dp"
                android:layout_margin="4dp"
                android:layout_weight="1"
                style="?buttonBarButtonStyle"
                android:background="@drawable/customer_rprt_bt01"
                android:fontFamily="@font/montserrat_medium1"
                android:text="Tool kit"
                android:textAllCaps="false"
                android:textSize="12sp" />

            <Button
                android:id="@+id/bt_cvmudguard"
                android:layout_width="0dp"
                android:layout_height="40dp"
                android:layout_margin="4dp"
                android:layout_weight="1"
                style="?buttonBarButtonStyle"
                android:background="@drawable/customer_rprt_bt01"
                android:fontFamily="@font/montserrat_medium1"
                android:text="Mudguard"
                android:textAllCaps="false"
                android:textSize="12sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="3">
            <Button
                android:id="@+id/bt_cvkeychain"
                android:layout_width="0dp"
                android:layout_height="40dp"
                android:layout_margin="4dp"
                android:layout_weight="1"
                style="?buttonBarButtonStyle"
                android:background="@drawable/customer_rprt_bt01"
                android:fontFamily="@font/montserrat_medium1"
                android:text="Keychain"
                android:textAllCaps="false"
                android:textSize="12sp" />

            <Button
                android:id="@+id/bt_cvservicebook"
                android:layout_width="0dp"
                android:layout_height="40dp"
                android:layout_margin="4dp"
                android:layout_weight="1"
                style="?buttonBarButtonStyle"
                android:background="@drawable/customer_rprt_bt01"
                android:fontFamily="@font/montserrat_medium1"
                android:text="Service book"
                android:textAllCaps="false"
                android:textSize="12sp" />

            <Button
                android:id="@+id/bt_cvmats"
                android:layout_width="0dp"
                android:layout_height="40dp"
                android:layout_margin="4dp"
                android:layout_weight="1"
                style="?buttonBarButtonStyle"
                android:background="@drawable/customer_rprt_bt01"
                android:fontFamily="@font/montserrat_medium1"
                android:text="Mats"
                android:textAllCaps="false"
                android:textSize="12sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="3">
            <Button
                android:id="@+id/bt_cvwheelcover"
                android:layout_width="0dp"
                android:layout_height="40dp"
                android:layout_margin="4dp"
                android:layout_weight="1"
                style="?buttonBarButtonStyle"
                android:background="@drawable/customer_rprt_bt01"
                android:fontFamily="@font/montserrat_medium1"
                android:text="Wheel cover"
                android:textAllCaps="false"
                android:textSize="12sp" />

            <Button
                android:id="@+id/bt_cvlock"
                android:layout_width="0dp"
                android:layout_height="40dp"
                android:layout_margin="4dp"
                android:layout_weight="1"
                style="?buttonBarButtonStyle"
                android:background="@drawable/customer_rprt_bt01"
                android:fontFamily="@font/montserrat_medium1"
                android:text="Lock"
                android:textAllCaps="false"
                android:textSize="12sp" />

            <Button
                android:id="@+id/bt_cvjackhandle"
                android:layout_width="0dp"
                android:layout_height="40dp"
                android:layout_margin="4dp"
                android:layout_weight="1"
                style="?buttonBarButtonStyle"
                android:background="@drawable/customer_rprt_bt01"
                android:fontFamily="@font/montserrat_medium1"
                android:text="@string/jack_handle"
                android:textAllCaps="false"
                android:textSize="12sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="16dp"
            android:orientation="horizontal"
            android:weightSum="3">
            <Button
                android:id="@+id/bt_cvcarpet"
                android:layout_width="0dp"
                android:layout_height="40dp"
                android:layout_margin="4dp"
                android:layout_weight="1"
                style="?buttonBarButtonStyle"
                android:background="@drawable/customer_rprt_bt01"
                android:fontFamily="@font/montserrat_medium1"
                android:text="Carpet"
                android:textAllCaps="false"
                android:textSize="12sp" />

            <Button
                android:id="@+id/bt_cvstereopanel"
                android:layout_width="0dp"
                android:layout_height="40dp"
                android:layout_margin="4dp"
                android:layout_weight="1"
                style="?buttonBarButtonStyle"
                android:background="@drawable/customer_rprt_bt01"
                android:fontFamily="@font/montserrat_medium1"
                android:text="Stereo panel"
                android:textAllCaps="false"
                android:textSize="12sp" />

            <Button
                android:id="@+id/bt_cvspeakers"
                android:layout_width="0dp"
                android:layout_height="40dp"
                android:layout_margin="4dp"
                android:layout_weight="1"
                style="?buttonBarButtonStyle"
                android:background="@drawable/customer_rprt_bt01"
                android:fontFamily="@font/montserrat_medium1"
                android:text="Speakers"
                android:textAllCaps="false"
                android:textSize="12sp" />
        </LinearLayout>

</LinearLayout>